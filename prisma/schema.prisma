generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Component {
  id    Int @id @default(autoincrement())
  name  String
  desc  String
  parentId Int? @unique
  parent   Component?   @relation("BlogOwnerHistory", fields: [parentId], references: [id])
  children Component[]  @relation("BlogOwnerHistory")
}
